    <link rel='stylesheet' href='/admin/plugins/toast-master/css/jquery.toast.css'/>
    <link rel='stylesheet' href='/admin/plugins/morrisjs/morris.css'/>
    <link rel='stylesheet' href='/admin/plugins/chartist-js/dist/chartist.min.css'/>
